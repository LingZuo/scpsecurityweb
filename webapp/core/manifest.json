{
	"_version": "1.5.0",
	"sap.app": {
		"id": "sap.security.core",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.security.core.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.security.core.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			],
			"js": [
				{
					"uri": "lib/lodash.min.js"
				},
				{
					"uri": "lib/moment-with-locales.min.js"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.security.core.view",
				"controlId": "idAppCoreControl",
				"controlAggregation": "pages",
				"transition": "flip"
			},
			"routes": [
				{
					"pattern": "",
					"name": "application",
					"target": "main"
				}
			],
			"targets": {
				"main": {
					"viewPath": "sap.security.core.view",
					"viewName": "Main",
					"viewId": "securityViewMain",
					"controlAggregation": "pages",
					"viewLevel": 1
				},
				"notFound": {
					"viewPath": "sap.security.core.view",
					"viewName": "NotFound",
					"viewId": "securityViewNotFound",
					"controlAggregation": "pages"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp/core",
		"_version": "1.1.0"
	}
}