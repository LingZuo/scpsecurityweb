<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="idUnassignConfirmPopup" title="{i18n>unassignConfirm}" type="Message">
		<content>
			<Text text="{i18n>unassignConfirmMsg}"/>
		</content>
		<beginButton>
			<Button text="{i18n>unassign}" press="onConfirmUnassignment"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>cancel}" press="onPressCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>